<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/activity_XML_usuarios_linear">

    <TextView
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:id="@+id/textListarUsuariosEsquerda"
        android:text="@string/textListarUsuariosEsquerda"
        android:textStyle="normal|bold"
        android:textAlignment="textEnd" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textListarUsuariosDireita"
        android:layout_weight="1"
        android:textAlignment="viewStart"
        android:text="@string/textListarUsuariosDireita" />

</LinearLayout>
